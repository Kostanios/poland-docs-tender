{
  "kind": "collectionType",
  "collectionName": "document_names",
  "info": {
    "singularName": "document-name",
    "pluralName": "document-names",
    "displayName": "documentName",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "inputType": {
      "type": "enumeration",
      "enum": [
        "multiple",
        "single"
      ],
      "default": "single"
    },
    "order": {
      "type": "integer",
      "default": 1
    },
    "typical_documents": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::typical-document.typical-document",
      "inversedBy": "document_names"
    },
    "document_lists": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::document-list.document-list",
      "mappedBy": "document_names"
    },
    "document_group": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::document-group.document-group"
    }
  }
}
