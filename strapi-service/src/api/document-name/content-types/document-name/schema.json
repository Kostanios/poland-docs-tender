{
  "kind": "collectionType",
  "collectionName": "document_names",
  "info": {
    "singularName": "document-name",
    "pluralName": "document-names",
    "displayName": "documentName"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "inputType": {
      "type": "enumeration",
      "enum": [
        "multiple",
        "single"
      ],
      "default": "single"
    },
    "order": {
      "type": "integer",
      "default": 1
    },
    "document_file_list": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::document-file-list.document-file-list",
      "mappedBy": "document_name"
    }
  }
}
